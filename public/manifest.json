{
  "author": "RemNote",
  "name": "Save to RemNote (Twitter and Email)",
  "description": "Save tweets and emails directly to RemNote!",
  "enableOnMobile": false,
  "id": "save_to_remnote",
  "manifestVersion": 1,
  "repoUrl": "https://github.com/remnoteio/twtterbotplugin",
  "version": {
    "major": 0,
    "minor": 0,
    "patch": 6
  },
  "minimumRemNoteVersion": {
    "major": 1,
    "minor": 9,
    "patch": 5
  },
  "requestNative": true,
  "requiredScopes": [
    {
      "type": "All",
      "level": "ReadCreateModifyDelete"
    },
    {
      "type": "KnowledgeBaseInfo",
      "level": "Read"
    }
  ]
}
